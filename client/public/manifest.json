    {
      "src": "/icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable any"
    }
  ],
  "start_url": "/",
  "background_color": "#ffffff",
  "theme_color": "#1976d2",
  "display": "standalone",
  "orientation": "portrait-primary",
  "scope": "/",
  "lang": "en-US",
  "dir": "ltr",
  "categories": [
    "business",
    "finance",
    "productivity",
    "utilities"
  ],
  "screenshots": [
    {
      "src": "/screenshots/desktop-1.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Dashboard view showing expense analytics"
    },
    {
      "src": "/screenshots/mobile-1.png",
      "sizes": "360x640",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Mobile expense creation with OCR"
    }
  ],
  "shortcuts": [
    {
      "name": "Create Expense",
      "short_name": "New Expense",
      "description": "Quickly create a new expense with camera capture",
      "url": "/expenses/create",
      "icons": [
        {
          "src": "/icons/shortcut-expense.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Pending Approvals",
      "short_name": "Approvals",
      "description": "View expenses waiting for approval",
      "url": "/approvals",
      "icons": [
        {
          "src": "/icons/shortcut-approval.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Analytics",
      "short_name": "Reports",
      "description": "View expense analytics and reports",
      "url": "/analytics",
      "icons": [
        {
          "src": "/icons/shortcut-analytics.png",
          "sizes": "96x96"
        }
      ]
    }
  ],
  "related_applications": [
    {
      "platform": "play",
      "url": "https://play.google.com/store/apps/details?id=com.company.expensemanager",
      "id": "com.company.expensemanager"
    },
    {
      "platform": "itunes",
      "url": "https://apps.apple.com/app/expense-manager/id123456789"
    }
  ],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "handle_links": "preferred",
  "launch_handler": {
    "client_mode": ["navigate-existing", "auto"]
  },
  "share_target": {
    "action": "/expenses/create",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "description",
      "url": "url",
      "files": [
        {
          "name": "receipt",
          "accept": ["image/*", "application/pdf"]
        }
      ]
    }
  },
  "protocol_handlers": [
    {
      "protocol": "web+expense",
      "url": "/expenses/create?data=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/expenses/import",
      "accept": {
        "application/pdf": [".pdf"],
        "image/*": [".jpg", ".jpeg", ".png", ".gif", ".webp"]
      }
    }
  ]
}